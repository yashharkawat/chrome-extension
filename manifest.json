{
    "name":"i18n namespace detector",
    "version":"1.0",
    "manifest_version":3,
    "description":"A tool to detect i18n labels",
    "permissions":["activeTab","tabs","storage","scripting"],
    "devtools_page":"devtools.html",
    "action":{
        "default_popup":"popup.html"
    },
    "content_scripts":[
        {
        "matches":["<all_urls>"],
        "js":["content.js"]
    }   
    ]
}

