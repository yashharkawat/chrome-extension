{
    "name":"dev tools",
    "version":"1.0",
    "manifest_version":3,
    "description":"dev tools basics",
    "permissions":["activeTab","tabs","storage","scripting"],
    "devtools_page":"devtools.html",
    "action":{
        "default_popup":"popup.html"
    },
    "content_scripts":[
        {
        "matches":["<all_urls>"],
        "js":["content.js"]
    }   
    ]
}

